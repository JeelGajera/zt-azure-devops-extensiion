{
    "manifestVersion": 1,
    "id": "zt-azure-devops",
    "name": "ZeroThreat AI DAST Scanner",
    "version": "0.0.2",
    "publisher": "ZeroThreatAIDASTScanner",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Scan for security issues in web applications/APIs",
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "tags": [
        "DAST",
        "AppSec",
        "Security",
        "build",
        "release"
    ],
    "branding": {
        "color": "rgb(240,242,255)",
        "theme": "light"
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/zerothreatai/azure-devops#zero-threat-ai-powered-security-scanner"
        },
        "license": {
            "uri": "https://github.com/zerothreatai/azure-devops/blob/main/LICENSE"
        },
        "support": {
            "uri": "https://zerothreat.ai/"
        },
        "issues": {
            "uri": "https://github.com/zerothreatai/azure-devops/issues"
        }
    },
    "icons": {
        "default": "images/zt_logo.png"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/zerothreatai/azure-devops"
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "zt-azure-devops",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}

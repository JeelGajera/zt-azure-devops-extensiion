{
    "manifestVersion": 1,
    "id": "zt-azure-devops",
    "name": "ZeroThreat AI DAST Scanner",
    "version": "0.0.2",
    "publisher": "ZeroThreatAIDASTScanner",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Scan for security issues in web applications/APIs",
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "zt-azure-devops",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}
